    <sys_script_client action="INSERT_OR_UPDATE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>onSubmit client script to validate that the end_time field is not before the start_time in the Schedule form</description>
        <field/>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>validation_enddate_startdate</name>
        <order/>
        <script><![CDATA[function onSubmit() {
   //Type appropriate comment here, and begin script below
   var startdate=g_form.getValue('x_start_date');
   var enddate= g_form.getValue('x_end_date');

   if(startdate && enddate){
   if((enddate<=startdate) ){
	g_form.addErrorMessage("End Date can be smaller then Start Date");
	return false;
   }
   }
   
}]]></script>
